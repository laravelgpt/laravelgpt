# LaravelGPT Environment Variables
# Copy this file to .laravelgpt.env and fill in your API keys

# OpenAI API Key (required for OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Google API Key (required for Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# Perplexity API Key (required for Perplexity models)
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Anthropic API Key (required for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Telemetry Configuration
TELEMETRY_ENABLED=true
TELEMETRY_DIAGNOSTICS=false

# IDE Configuration
IDE=cursor

GEMINI_API_KEY="your-api-key-here"
OPENROUTER_API_KEY="your-api-key-here"
CLICKUP_API_TOKEN="your-api-token-here" # https://app.clickup.com/settings/apps
GROQ_API_KEY="your-api-key-here"
MODELBOX_API_KEY="your-api-key-here"
FIRECRAWL_API_KEY="your-api-key-here"
